# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# Box API設定
BOX_CLIENT_ID=your_box_client_id
BOX_CLIENT_SECRET=your_box_client_secret
BOX_ENTERPRISE_ID=your_enterprise_id
BOX_JWT_PRIVATE_KEY=your_private_key
BOX_JWT_PRIVATE_KEY_PASSPHRASE=your_passphrase
BOX_PUBLIC_KEY_ID=your_public_key_id
BOX_PROJECTS_ROOT_FOLDER_ID=your_folder_id

# Box Sign機能制御（Box Sign APIが有効化されるまで false に設定）
BOX_SIGN_ENABLED=false

# NextAuth設定
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret

# その他のAPI設定
NEXT_PUBLIC_ADMIN_EMAILS=admin@example.com